<!-- formulario de alta de tarea -->
<form action="{BASE_URL}addZapa" method="POST" class="my-4">
<div class="row">
        <div class="col-9">
            <div class="form-group">
             
   
    <input placeholder="Modelo" type="text" name="Modelo" id="Modelo" required>
    <input placeholder="Precio" type="number" name="Precio" id="Precio" required>
    <input placeholder="Stock" type="number" name="Stock" id="Stock" required>
   

                        
                                          
                 
    <button type="submit"
     class="btn btn-primary mt-2">
     Guardar</button>
</form>